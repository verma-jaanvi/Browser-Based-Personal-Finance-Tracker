{
  "builds": [
    {
      "src": "src/pages/index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/src/pages/dashboard.html" },
    { "source": "/styles.css", "destination": "/src/pages/styles.css" },
    { "source": "/script.js", "destination": "/src/pages/script.js" },
    { "source": "/firebase-config.js", "destination": "/src/pages/firebase-config.js" },
    { "source": "/budgetLogic.js", "destination": "/src/pages/budgetLogic.js" }
  ]
}
